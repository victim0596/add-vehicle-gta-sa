<Window x:Class="addVehicle.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:addVehicle.Windows" 
        mc:Ignorable="d"
        Title="GTA SA New Vehicle" Height="550" Width="800" Background="Black"
        Closing="Window_Closing" ResizeMode="CanMinimize"
        >
    <Grid>
        <DockPanel>
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="_Settings" Click="openSettings"/>
            </Menu>
            <StackPanel></StackPanel>
        </DockPanel>
        <Frame Source="../Pages/HomePage.xaml" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
